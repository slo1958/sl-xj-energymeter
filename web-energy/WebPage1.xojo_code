#tag Events Timer1
	#tag Event
		Sub Run()
		  
		  UpdateRefreshProcess
		  
		  if self.RefreshRate < 0 then return
		  
		  if self.RequestStatus <> 0 then Return
		  
		  
		  var url as string = app.config.GetMeterURL()
		  
		  if url.left(4) <> "http" then
		    url = "http://" + url
		  end if
		  
		  self.RequestStatus = 1
		  
		  self.lbl_message.text = "Sending request to " + url + "..."
		  
		  URLConnection1.Send("GET",url,10)
		  
		  Return
		  
		End Sub
