#tag EndWebPage

#tag WindowCode
	#tag Event
		Sub Opening()
		  self.RequestStatus = 0
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Function FormatkW(d as Double) As String
		  
		  return format(d, "000000.000")
		  
		  
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub UpdateRefreshProcess()
		  if self.RefreshRate < 0 or self.RequestStatus < 0 then
		    timer1.RunMode = WebTimer.RunModes.Off
		    Return
		    
		  end if
		  
		  
		  if RequestStatus <> 0 then
		    timer1.RunMode = WebTimer.RunModes.Single
		    timer1.Period = 1000 
		    Return
		    
		  end if
		  
		  timer1.Period = RefreshRate * 1000
		  timer1.RunMode = WebTimer.RunModes.Multiple
		  
		  Return
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub UpdateUserInterface()
		  
		  self.lbl_message.text = self.lastError
		   
		  if self.lastTelegram = nil then Return
		  
		  Canvas181.SetIndex(self.lastTelegram.GetCounterValue("1.8.1"))
		  Canvas182.SetIndex(self.lastTelegram.GetCounterValue("1.8.2"))
		  Canvas281.SetIndex(self.lastTelegram.GetCounterValue("2.8.1"))
		  Canvas282.SetIndex(self.lastTelegram.GetCounterValue("2.8.2"))
		  
		  
		  canvas21.SetIndex(self.lastTelegram.GetCounterValue("21.7.0")*1000)
		  canvas41.SetIndex(self.lastTelegram.GetCounterValue("41.7.0")*1000)
		  canvas61.SetIndex(self.lastTelegram.GetCounterValue("61.7.0")*1000)
		  canvas22.SetIndex(self.lastTelegram.GetCounterValue("22.7.0")*1000)
		  canvas42.SetIndex(self.lastTelegram.GetCounterValue("42.7.0")*1000)
		  canvas62.SetIndex(self.lastTelegram.GetCounterValue("62.7.0")*1000)
		  
		  lbl_message.text = self.lastTelegram.GetCounterRawValue("96.14.0")
		  Return
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		lastError As String
	#tag EndProperty

	#tag Property, Flags = &h0
		lastTelegram As clMeterTelegram
	#tag EndProperty

	#tag Property, Flags = &h0
		RefreshRate As Integer
	#tag EndProperty

	#tag Property, Flags = &h0
		RequestStatus As Integer
	#tag EndProperty


#tag EndWindowCode

#tag Events pm_refresh
	#tag Event
		Sub SelectionChanged(item As WebMenuItem)
		  
		  select case me.SelectedRowIndex
		    
		  case 0
		    self.RefreshRate = -1
		    
		    self.RequestStatus = 0
		    self.lastError = ""
		    
		  case 1
		    self.RefreshRate = 2
		    
		  case 2 
		    self.RefreshRate = 5
		    
		  case else
		    self.RefreshRate = 60
		    
		  end Select
		  
		  
		  UpdateRefreshProcess()
		  UpdateUserInterface()
		  
		End Sub
